{
  "name": "opendiscourse-ingestion",
  "version": "1.0.0",
  "description": "Document ingestion system for OpenDiscourse",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node --experimental-specifier-resolution=node dist/scripts/ingest_govinfo_bulkdata.js",
    "dev": "ts-node --esm src/scripts/ingest_govinfo_bulkdata.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "ingest:optimized": "tsx scripts/optimized_ingest.ts",
    "ingest:collection": "tsx scripts/optimized_ingest.ts",
    "ingest:all": "tsx scripts/optimized_ingest.ts all",
    "ingest:enhanced": "tsx scripts/test_enhanced_processor.ts",
    "bulkdata:ingest": "tsx scripts/ingest_govinfo_bulkdata.ts",
    "bulkdata:process": "tsx scripts/process_document.ts",
    "test:processor": "tsx scripts/test_optimized_processor.ts",
    "test:enhanced": "tsx scripts/test_enhanced_processor.ts"
  },
  "dependencies": {
    "@tensorflow-models/universal-sentence-encoder": "^1.3.3",
    "@tensorflow/tfjs-node": "^4.2.0",
    "@types/sqlite3": "^5.1.0",
    "compromise": "^14.4.0",
    "dotenv": "^16.0.3",
    "generic-pool": "^3.9.0",
    "natural": "^6.5.4",
    "node-fetch": "^3.3.0",
    "openai": "^4.0.0",
    "pg": "^8.10.0",
    "pgvector": "^0.2.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.6",
    "tsx": "^4.7.0",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.11.18",
    "@types/tar": "^6.1.13",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
